#include <stdio.h>
#include <stdlib.h>

int main()
{
    printf("enter the size of element");
    int n ,i,j,temp;
    scanf("%d",&n);
    int a[n];
    printf("enter elements of array\n");
    for(i=0;i<n;i++){
	printf("enter the "%d"th element",i+1);
        scanf("%d",&a[i]);}
    for(i=0;i<n;i++)
    {
        for(j=0;j<n-i-1;j++)
        {
            if(a[j]>a[j+1])
            {
                temp=a[j];
                a[j]=a[j+1];
                a[j+1]=temp;
            }
        }
    }
    for(i=0;i<n;i++)
        printf("%d ",a[i]);
    return 0;
}
